        token_1_text_text: A6
        token_3_text_text: $100
        _major_fill: ${colour/xxp/BLACK}
        _minor_fill: ${colour/xxp/WHITE}
        _related_fill: ${colour/x11/gray43}
        _share_pct_text_fill_x: ${colour/xxp/WHITE}
      token:
        ELEMENTS: ${TEMPLATE_cross/.}
        EMBED_ELEMENTS: ${TEMPLATE_cross_embed/.}
        index: "$[index_of (6)]"

    ABC:
      _:
        long_name_text_text: |
             Atlanta, Birmingham
             & Coast Railroad
        long_name_text_fill: ${colour/xxp/BLACK}
        short_name_text_text: ABC
        token_1_text_text: F7
        _major_fill: ${colour/xxp/D_GREEN}
        _minor_fill: ${colour/xxp/D_GREEN}
        _related_fill: ${colour/hollasch/cinnabar_green}
        name_stripe_box_suppress: True
      token:
        _:
          short_name_text_fill: $[black_or_white (${_/_major_fill})]
          short_name_text_size: 10
          short_name_text_x_inset: 0
          #short_name_text_y_inset: -2
        ELEMENTS: ${TEMPLATE_solid/.}
        EMBED_ELEMENTS: ${TEMPLATE_solid_embed/.}
        index: $[index_of (5)]

marker:
  index:
    round:
      token:
        ELEMENTS:
          - token_interior_box
          - outer_circle
          - middle_circle
          - inner_circle

        index: $[index_of (1)]
      outer_circle:
        fill: ${colour/galatolol/yellow}
      middle_circle:
        fill: ${colour/galatolol/green}
      inner_circle:
        fill: ${colour/galatolol/brown}

    p10:
      token:
        _:
          name_stripe_box_suppress: True
          short_name_text_text: "+10"
          short_name_text_size: 10
          outer_circle_fill: ${colour/wikipedia/white}
          middle_circle_fill: ${colour/wikipedia/white}
        ELEMENTS: ${TEMPLATE_ring/.}
        index: "$[index_of (2)]"

    p20:
      token:
        _:
          name_stripe_box_suppress: True
          short_name_text_text: "+20"
          short_name_text_size: 10
          outer_circle_fill: ${colour/wikipedia/white}
          middle_circle_fill: ${colour/wikipedia/white}
        ELEMENTS: ${TEMPLATE_ring/.}
        index: "$[index_of (1)]"

    p40:
      token:
        _:
          name_stripe_box_suppress: True
          short_name_text_text: "+40"
          short_name_text_size: 10
          outer_circle_fill: ${colour/wikipedia/white}
          middle_circle_fill: ${colour/wikipedia/white}
        ELEMENTS: ${TEMPLATE_ring/.}
        index: "$[index_of (1)]"

private:
  _:
    long_name_text_size: 11
    money_bar_box_y: 13
    closes_text_size: 8
    closes_text_text: "Closed by 5"
    power_text_size: 8
    revenue_text_x: 168
    asset_interior_box_fill: ${colour/scale/yellow_0}

  index:

    TR:
      _:
        long_name_text_text: |
            Tuscumbia Railway
        power_text_text: ""
        cost_text_text: "Cost: $20"
        revenue_text_text: "Revenue: $5"

    SNAR:
      _:
        long_name_text_text: |
            South & North Alabama
            Railroad
        power_text_text: |
            Owning company may place a +10 marker in
            Gadsden, Anniston, Oxmoor, Birmingham or
            Tuscaloosa if it can reach that city by train.
            For that company only, that city is worth 10 more.
            The marker is removed in Phase 6.
        cost_text_text: "Cost: $40"
        revenue_text_text: "Revenue: $10"

    BSLC:
      _:
        long_name_text_text: |
            Brown & Sons Lumber Co.
        power_text_text: |
            Owning company may lay the #445 tile
            in a swamp hex.
            Doesn't have to be connected,
            doesn't count as track action.
            That tile cannot be upgraded.
        cost_text_text: "Cost: $70"
        revenue_text_text: "Revenue: $15"

    C&A:
      _:
        long_name_text_text: |
            Memphis & Charleston
            Railroad
        power_text_text: |
            Owning company gets the +20 and +40 markers.
            Each OR they may be assigned to its trains.
            +20 if route includes Atlanta and Birmingham.
            +40 if route includes Nashville and Mobile.
            If the presidency changes, the markers are removed.
        cost_text_text: "Cost: $100"
        revenue_text_text: "Revenue: $20"

    NDY:
      _:
        closes_text_text: "Closed by 5 or when the ability used"
        closes_text_x_inset: -1
        long_name_text_text: |
            New Decatur Yards
        power_text_text: |
            Owning company may purchase one new train
            with a discount of 50%.
        cost_text_text: "Cost: $120"
        revenue_text_text: "Revenue: $20"

