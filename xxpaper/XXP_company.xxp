      x: 42
      y: 228
      h_center: 0
      v_center: 0
      fill: ${colour/xxp/BLACK}

    token_2_circle:
      suppress: $["${token_2_text/text}" == ""]
      x: 42
      y: $[${token_5_circle/y} + ((${token_1_circle/y} - ${token_5_circle/y}) * 3.0 / 4.0)]
      radius: 18
      width: 0.5
      stroke: ${colour/xxp/BLACK}
      fill:  ${colour/xxp/WHITE}

    token_2_text:
