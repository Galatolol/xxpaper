    index:
      "1":

    cutline_box: ${DEFAULT/charter_tile}

    outside_box:
      x_inset: 10
      y_inset: 10
      x: 242
      y: 332
      radius: 30
      stroke: ${colour/xxp/BLACK}
      fill: ${colour/hollasch/eggshell}
      width: 4

    title_box:
      x_inset: 10
      y_inset: 282
      x: 242
      y: 60
      radius: 30
      stroke: ${colour/xxp/BLACK}
      width: 4
      fill: ${_/_related_fill}

    token_embed:
      embed: token
      x_inset: 18
      y_inset: 290

    label_text:
      typeface: ${DEFAULT/typeface_italic}
      size: 13
      x_inset: 10
      y_inset: 260
      text: Assets, Treasury & Trains
      x: $[${outside_box/x} / 2]
      y: 0
      h_center: 0
      v_center: 0
      fill: ${colour/xxp/BLACK}

    top_note_text:
      suppress: True
      typeface: ${DEFAULT/typeface}
      size: 8
      x_inset: 10
      y_inset: 245
      x: $[${outside_box/x} / 2]
      y: 0
      h_center: 0
      v_center: 1
      fill: ${colour/xxp/BLACK}

    bottom_note_text:
      suppress: False
      text: |
        Purchase price of shares goes to bank.
        Pool shares pay dividends to treasury.
      typeface: ${DEFAULT/typeface}
      size: 8
      x_inset: 10
      y_inset: 20
      x: $[${outside_box/x} / 2]
      y: 0
      h_center: 0
      v_center: -1
      fill: ${colour/xxp/BLACK}

    long_name_text:
      h_center: 0
      v_center: 0
      size: 16
      x: 143
      y: 305

    token_1_circle:
      suppress: $["${token_1_text/text}" == ""]
      x: 42
      y: 232
      radius: 18
      width: 0.5
      stroke: ${colour/xxp/BLACK}
      fill:  ${colour/xxp/WHITE}

    token_1_text:
      suppress: $["${token_1_text/text}" == ""]
