  share:

    CUT_ELEMENT: cutline_box

    ELEMENTS:
      - asset_interior_box
      - rotate
      - side_bar_box
      - type_bar_box
      - share_desc_text
      - share_pct_text
      - share_num_text
      - long_name_text
      - comment_text
      - token_2a_embed
      - token_3a_embed
      - token_1_embed
      - token_3b_embed
      - token_2b_embed

    index:
      "1":
        type_bar_box:
          suppress: False
        share_num_text:
          text: 2 Shares
        share_desc_text:
          suppress: False
        share_ptc_text:
          text: 20%
        token_1_embed:
          suppress: True
        token_2a_embed:
          suppress: False
        token_2b_embed:
          suppress: False
      "2":
      "3":
      "4":
      "5":
      "6":
      "7":
      "8":
      "9":

    cutline_box: ${DEFAULT/asset_tile}

    long_name_text:
      size: 14
      x: 101
      y: 84

    side_bar_box:
      x: 36
      y: ${asset_interior_box/x}
      fill: ${_/_related_fill}

    type_bar_box:
      x: ${asset_interior_box/y}
      y: 11
      fill: ${colour/xxp/DIRECTOR}
      suppress: True

    share_num_text:
      x: 160
      y: 1.5
      h_center: -1
      v_center: 1
      typeface: ${DEFAULT/typeface_italic}
      size: 11
      text: 1 Share

    share_pct_text:
      text: 10%
      x: 18
      y: 1.5
      h_center: 0
      v_center: -1
      typeface: ${DEFAULT/typeface_italic}
      size: 11

    share_desc_text:
      text: Director
      x: 64
      y: 1.5
      h_center: 0
      v_center: 1
      typeface: ${DEFAULT/typeface_italic}
      size: 11
      suppress: True

    comment_text:
      x: 101
      y: 16
      h_center: 0
      v_center: -1
      typeface: ${DEFAULT/typeface_italic}
      size: 8

    token_1_embed:
      suppress: False
      embed: token
      x_inset: -4
      y_inset: $[(${token_2a_embed/y_inset} + ${token_2b_embed/y_inset}) / 2.0]

    token_2a_embed:
      suppress: True
      embed: token
      x_inset: -4
      y_inset: $[(${side_bar_box/y} - 10) / 2.0 + ${type_bar_box/y}]

    token_2b_embed:
      suppress: True
      embed: token
      x_inset: -4
      dfgfgy_inset: 20
      y_inset: 12

    token_3a_embed:
      suppress: True
      embed: token
      x_inset: -4
      y_inset: $[(${token_2a_embed/y_inset} + ${token_1_embed/y_inset}) / 2.0]

    token_3b_embed:
      suppress: True
      embed: token
      x_inset: -4
      y_inset: $[(${token_2b_embed/y_inset} + ${token_1_embed/y_inset}) / 2.0]

  token:

    index:

    ELEMENTS:
      - token_interior_box
      - top_half_box
      - bottom_half_box
      - name_stripe_box
      - short_name_text

    EMBED_ELEMENTS:
      - top_half_box
      - bottom_half_box
      - name_stripe_box
      - short_name_text
      - outline_circle

    EMBED_CUT_ELEMENT: outline_circle

    _include_:
      - XXP_token_objects.xxp
