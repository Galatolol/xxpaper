private:

  index:

    P1:
      _:
        long_name_text_text: |
            Wrocław-Oława Railroad
        cost_text_text: "Cost: $20"
        revenue_text_text: "Revenue: $5"

    P2:
      _:
        long_name_text_text: |
            Industrialisation of Łódź
        power_text_text: |
            Owning company may place
            a +$20 token in Łódź (G13).
            Increases the value of the city
            for all companies.
        cost_text_text: "Cost: $40"
        revenue_text_text: "Revenue: $10"

    P3:
      _:
        long_name_text_text: |
            Local Railroad
        power_text_text: |
            Owning company may upgrade
            a yellow town using an IPL1 tile.
            This is in addition to its
            normal tile placement.
        cost_text_text: "Cost: $60"
        revenue_text_text: "Revenue: $15"

    P4:
      _:
        closes_text_text: "Closes in: see below"
        long_name_text_text: |
            Bridge Company
        power_text_text: |
            Owning company ignores
            terrain costs.
        cost_text_text: "Cost: $80"
        revenue_text_text: "Revenue: $20"

    P5:
      _:
        long_name_text_text: |
            Baltic Shipping
        power_text_text: |
            Owning company may place a $40
            token in any port (B4, E1, G3).
            Owning company may use it
            as a stop for its trains.
        cost_text_text: "Cost: $100"
        revenue_text_text: "Revenue: $25"

    P6:
      _:
        closes_text_text: "Closes immediately"
        long_name_text_text: |
            Warsaw-Radom Railroad
        power_text_text: |
            Owning player immediately takes the
            presidency of Minor 8, places its home
            station (I11), and puts the bid into its
            treasury.
            Par price is the highest yellow par 
            value that is not more than half the bid. 
            Then Minor 8 buys a train.
        power_text_size: 8
        cost_text_text: "Cost: $120"
        revenue_text_text: "Revenue: $0"


